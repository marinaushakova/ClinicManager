                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT visit.id, visit.date, visit.patient_id AS patientID, patient.lname, patient.fname, patient.minit,
	doctor.lname, doctor.fname, doctor.minit,
                  nurse.lname, nurse.fname, nurse.minit, symptoms, 
	initial_diagnosis, final_diagnosis , test.name, 
	orderedtest.result_date, orderedtest.result	 
FROM person AS patient JOIN visit ON patient.id = visit.patient_id
    JOIN person AS doctor ON doctor.id = visit.doctor_id
    JOIN person AS nurse ON nurse.id = visit.nurse_id
    FULL JOIN orderedtest ON orderedtest.visit_id = visit.id
    FULL  JOIN test ON orderedtest.test_id = test.id
WHERE [date] &gt;= @startDate AND [date] &lt; @endDate</CommandText>
